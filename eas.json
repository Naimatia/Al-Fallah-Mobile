{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "@secret:EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY": "@secret:EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_WEATHER_API_KEY": "@secret:EXPO_PUBLIC_WEATHER_API_KEY"
      }
    },
    "preview2": {
      "distribution": "internal",
      "extends": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "@secret:EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY": "@secret:EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "EXPO_PUBLIC_WEATHER_API_KEY": "@secret:EXPO_PUBLIC_WEATHER_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}